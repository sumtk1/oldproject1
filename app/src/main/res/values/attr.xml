        <attr name="aret_text_num" format="integer"/>
        <attr name="aret_desc" format="string"/>
        <attr name="aret_padding_left" format="dimension"/>
        <attr name="aret_hint" format="string"/>
    </declare-styleable>

    <!-- 手势锁 -->
    <attr name="color_no_finger_inner_circle" format="color" />
